sole.log('44', _sub);\r\n            \r\n            for(const s of _sub){\r\n                sub.push(s.name);\r\n            }\r\n        }).then(\r\n            alert(\"Subjects :\\n\"+sub)\r\n        )\r\n    }\r\n\r\n    render() {\r\n        return(\r\n            <div>\r\n                <h2 align=\"center\">Course Details</h2>\r\n                <form>\r\n                <table align=\"center\">\r\n                        <tbody>\r\n                            <tr>\r\n                                <th>Name</th>\r\n                                <th>Code</th>\r\n                                <th>Pass Mark</th>\r\n                                <th>Lecture in Charge</th>\r\n                            </tr>\r\n                            \r\n                                {this.state.courses}\r\n                            \r\n                        </tbody>\r\n                    </table>\r\n                </form>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"9d88c800bd5a8ef177d79dc880e28ce7","cacheData":{"env":{}}}